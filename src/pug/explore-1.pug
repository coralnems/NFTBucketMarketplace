        // Breadcrumb 
        .breadcrumb-wrapper
            .container
                .breadcrumb-content
                    h2.breadcrumb-title Explore One
                    nav(aria-label="breadcrumb")
                        ol.breadcrumb.justify-content-center
                            li.breadcrumb-item
                                a(href="index.html") Home
                            li.breadcrumb-item.active(aria-current="page") Explore

        .divider

        // Explore Items 
        .explore-items-wrapper
            .container
                .row.g-4.align-items-end
                    .col-12.col-sm-6.col-lg-3
                        h5 Status
                        select.filter-select.bg-gray.w-100
                            option(selected, value="buy-now") Buy Now
                            option(value="on-auction") On Auction
                            option(value="new") New
                            option(value="featured") Featured

                    .col-12.col-sm-6.col-lg-3
                        h5 Categories
                        select.filter-select2.bg-gray.w-100
                            option(selected, value="1") Art
                            option(value="2") Cards
                            option(value="3") Collectibles
                            option(value="4") Domain
                            option(value="5") Music
                            option(value="6") Memes
                            option(value="7") Photos
                            option(value="8") Sports
                            option(value="9") Videos
                            option(value="10") Vitual Worlds

                    .col-12.col-sm-6.col-lg-3
                        h5 Items
                        select.filter-select.bg-gray.w-100
                            option(selected, value="1") All
                            option(value="2") Single
                            option(value="3") Bundle

                    .col-12.col-sm-6.col-lg-3
                        h5 Sort By
                        select.filter-select.bg-gray.w-100
                            option(selected, value="1") Recently Added
                            option(value="2") Recently Sold
                            option(value="3") Ending Soon

                    .col-12.col-sm-6.col-lg-3
                        h5 Price
                        input#filterRange(type="text", name="filterByValue", value="", data-min="100", data-max="1000", data-from="300", data-to="750", data-type="double", data-prefix="", data-grid="false")

                    .col-12.col-sm-6.col-lg-3
                        h5 Ratings
                        select.filter-select.bg-gray.w-100
                            option(selected, value="1") 5 Star
                            option(value="2") 4 Star &amp; Above
                            option(value="3") 3 Star &amp; Above
                            option(value="4") 2 Star &amp; Above
                            option(value="5") 1 Star &amp; Above

                    .col-12.col-sm-6.col-lg-3
                        h5 Chains
