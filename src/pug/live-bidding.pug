doctype html
html(lang="en", data-theme="dark")
    head
        include inc/head.pug

    body
        include inc/loader.pug

        include inc/header.pug

        // Breadcrumb 
        .breadcrumb-wrapper
            .container
                .breadcrumb-content
                    .d-flex.align-items-center.justify-content-center
                        .spinner-grow.text-danger.me-1(role="status")
                            span.visually-hidden Loading...
                        h2.breadcrumb-title.mb-0 Live Bids

                    nav(aria-label="breadcrumb")
                        ol.breadcrumb.justify-content-center
                            li.breadcrumb-item
                                a(href="index.html") Home
                            li.breadcrumb-item.active(aria-current="page") Live Bids

        .divider

        .live-bids-wrapper
            .container
                .row.g-4.justify-content-center
                    .col-12.col-sm-6.col-lg-4.col-xl-3
                        .nft-card.card.shadow-sm
                            .card-body
                                .img-wrap
                                    img(src="img/bg-img/9.jpg", alt="")

                                    // Badge 
                                    .badge.bg-dark.position-absolute
                                        img(src="img/core-img/fire.png", alt="")
                                        | New Bid

                                    // Dropdown 
                                    .dropdown
                                        button.btn.dropdown-toggle.rounded-pill.shadow-sm#dropdowne22(type="button" data-bs-toggle="dropdown" aria-expanded="false")
                                            i.bi.bi-three-dots-vertical
                                        
                                        // Dropdown Menu 
                                        ul.dropdown-menu.dropdown-menu-end(aria-labelledby="dropdowne22")
                                            li
                                                a.dropdown-item(href="#")
                                                    i.me-1.bi.bi-share
                                                    | Share
                                            li
                                                a.dropdown-item(href="#")
                                                    i.me-1.bi.bi-flag
                                                    | Report
                                            li
                                                a.dropdown-item(href="#")
                                                    i.me-1.bi.bi-bookmark
                                                    | Bookmark

                                    // Bid End 
                                    .bid-ends(data-date="March 31, 2025 14:19:54")
                                        div
                                            span.days
                                            span Days
                                        div
                                            span.hours
                                            span Hours
                                        div
                                            span.minutes
                                            span Min
                                        div
                                            span.seconds
                                            span Sec
                                
                                // Others Info 
                                .row.gx-2.align-items-center.mt-3
                                    .col-8
                                        span.d-block.fz-12
                                            i.bi.bi-bag.me-1
                                            | 3 stock available
                                    .col-4.text-end
                                        button.wishlist-btn(type="button")
                                            i.bi
                                
